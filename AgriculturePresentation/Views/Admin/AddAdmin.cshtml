@model EntityLayer.Concrete.Admin
@{
    ViewData["Title"] = "AddAdmin";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="col-12 grid-margin stretch-card">
  <div class="card">
    <div class="card-body">
      <h4 class="card-title">Yeni Admin Oluştur</h4>
      <form class="forms-sample" method="post">
        <div class="form-group">
          <label for="exampleInputName1">Kullanıcı Adı</label>
          <input type="text" class="form-control" placeholder="Kullanıcı Adı" asp-for="Username">
        </div>
        <div class="form-group">
          <label for="exampleInputEmail3">Şifre</label>
          <input type="password" class="form-control" placeholder="şifre" asp-for="Password">
        </div>
        <button type="submit" class="btn btn-primary me-2">Kaydet</button>
      </form>
    </div>
  </div>
</div>

